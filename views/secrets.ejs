    <i class="fas fa-key fa-6x"></i>
    <h1 class="display-3">You've Discovered My Secret!</h1>
    
    <hr>
    <% usersWithSecrets.map((user)=> { %>
      <h3 class="fw-bold"><%=user.secret%><br></h3>
    <% }) %>

    <hr>
    
    <a class="mt-5 btn btn-outline-dark btn-lg" href="/logout" role="button">Log Out</a>
    <a class="mt-5 btn btn-dark btn-lg" href="/submit" role="button">Submit a Secret</a>
  </div>
</div>

<%- include('partials/footer') %>
